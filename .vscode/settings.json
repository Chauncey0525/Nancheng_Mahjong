{
  // 配置文件关联，避免在文本编辑器中打开二进制文件
  "files.associations": {
    "*.db": "binary",
    "*.sqlite": "binary",
    "*.sqlite3": "binary"
  },
  
  // 排除文件，不在文件浏览器中显示（数据库文件已取消隐藏）
  "files.exclude": {
    "**/node_modules": true
  },
  
  // 搜索时排除的文件（数据库文件已取消隐藏）
  "search.exclude": {
    "**/node_modules": true
  }
}
